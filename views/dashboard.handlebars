<div class="dashboard-holder">

  <div class="blogs">
    <p class="update-blogs">Here you can manage your blogs</p>
    {{#each blogs }}

    <a href="dashboard/update/{{id}}">
      <div class="blog-block">
        <div class="update-blog-title">{{this.title}}</div>
        <div class="update-blog-date">{{format_date createdAt}}</div>
      </div>
    </a>
    {{/each}}

  </div>
    <a href="dashboard/newpost" class="btn newpost-btn">New post</a>
</div>
